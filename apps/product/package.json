{
  "name": "@nstack/news-api",
  "version": "0.0.1",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:migrate": "docker-compose -f docker-compose.migrate.yml up -d",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d",
    "docker:seed": "docker exec -it nest-api npm run seed",
    "docker": "docker-compose up -d",
    "docker:build": "docker-compose build"
  },
  "dependencies": {
    "@nestjs/apollo": "10.0.19",
    "@nestjs/common": "9.0.5",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.0.5",
    "@nestjs/graphql": "10.0.20",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "9.0.1",
    "@nestjs/mongoose": "^9.2.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "9.0.5",
    "@nestjs/swagger": "6.0.4",
    "@nestjs/websockets": "^9.3.5",
    "amqp-connection-manager": "^4.1.10",
    "amqplib": "^0.10.3",
    "apollo-server-express": "3.10.0",
    "axios": "^0.27.2",
    "bcrypt": "5.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "express": "^4.18.2",
    "graphql": "16.5.0",
    "graphql-redis-subscriptions": "^2.6.0",
    "graphql-scalars": "1.17.0",
    "graphql-subscriptions": "1.2.1",
    "ioredis": "^5.3.1",
    "lodash": "^4.17.21",
    "mongodb": "^5.2.0",
    "mongoose": "^6.4.7",
    "mongoose-aggregate-paginate-v2": "^1.0.6",
    "mongoose-autopopulate": "^1.0.0",
    "mongoose-paginate-v2": "^1.7.1",
    "passport": "^0.6.0",
    "passport-jwt": "4.0.0",
    "passport-local": "^1.0.0",
    "pluralize": "^8.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.6",
    "socket.io": "^4.6.1"
  },
  "devDependencies": {
    "@nestjs/cli": "9.0.0",
    "@nestjs/schematics": "9.0.1",
    "@nestjs/testing": "9.0.5",
    "@types/bcrypt": "5.0.0",
    "@types/chance": "1.1.3",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.6",
    "@types/mongoose-aggregate-paginate-v2": "^1.0.6",
    "@types/node": "^18.0.3",
    "@types/passport-local": "^1.0.34",
    "@types/pluralize": "^0.0.29",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "chance": "1.1.8",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "28.1.3",
    "prettier": "2.7.1",
    "supertest": "6.2.4",
    "ts-jest": "28.0.7",
    "ts-loader": "9.3.1",
    "ts-morph": "^15.1.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}